pipeline {
  agent any
  stages {
    stage('Checkout') {
      steps {
        echo 'Checkout from github Completed .....'
      }
    }

    stage('Build') {
      steps {
        echo 'Build Completed .....'
      }
    }

    stage('Test') {
      steps {
        echo 'Test Completed .....'
      }
    }

    stage('Deploy') {
      steps {
        echo 'Deploy Completed .....'
      }
    }

  }
}